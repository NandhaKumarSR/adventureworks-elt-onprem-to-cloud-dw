{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-onprem-to-cloud-02-proj"
		},
		"adlsgen2ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adlsgen2ls'"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0917-161402-oisqu9im"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-adventureworks-proj02.vault.azure.net/"
		},
		"adlsgen2ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://onpremtocloudadls.dfs.core.windows.net/"
		},
		"onpremsqlserverls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "MACHINE\\SQLEXPRESS"
		},
		"onpremsqlserverls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorksLT2017"
		},
		"onpremsqlserverls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "nk"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-onprem-to-cloud-02-proj_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adlsgen2ls_accountKey": {
						"value": "[parameters('adlsgen2ls_accountKey')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"adlsgen2ls_properties_typeProperties_url": {
						"value": "[parameters('adlsgen2ls_properties_typeProperties_url')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"onpremsqlserverls_properties_typeProperties_server": {
						"value": "[parameters('onpremsqlserverls_properties_typeProperties_server')]"
					},
					"onpremsqlserverls_properties_typeProperties_database": {
						"value": "[parameters('onpremsqlserverls_properties_typeProperties_database')]"
					},
					"onpremsqlserverls_properties_typeProperties_userName": {
						"value": "[parameters('onpremsqlserverls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}